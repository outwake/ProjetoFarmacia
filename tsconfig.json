    /* Linguagem e Ambiente */
    "target": "es2020",                                  /* Define a versão do JavaScript gerado e inclui bibliotecas compatíveis. */
    // "lib": [],                                        /* Especifica um conjunto de arquivos de declaração que descrevem o ambiente de execução. */
    // "jsx": "preserve",                                /* Define como o código JSX será gerado. */
